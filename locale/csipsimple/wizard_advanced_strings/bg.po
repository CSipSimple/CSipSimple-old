msgctxt "w_advanced_display_name"
msgid "Account name"
msgstr "Име на профила"

msgctxt "w_advanced_display_name_desc"
msgid "Display name for this account"
msgstr "Показване на име за този профил"

msgctxt "w_advanced_caller_id"
msgid "Caller ID"
msgstr "Идентификатор на позвъняващ"

msgctxt "w_advanced_caller_id_desc"
msgid "Caller ID to send with calls"
msgstr "(Caller ID) Идентификатор на позвъняващия, използван при повикване"

msgctxt "w_advanced_username"
msgid "Username"
msgstr "Потребителско име"

msgctxt "w_advanced_username_desc"
msgid "Username for SIP server authentication"
msgstr "Потребителско име (акаунт код) за ауторизиране към SIP сървъра"

msgctxt "w_advanced_password"
msgid "Password"
msgstr "Парола"

msgctxt "w_advanced_password_desc"
msgid "Password for SIP server authentication"
msgstr "Парола за ауторизация към SIP сървъра"

msgctxt "w_advanced_use_tcp"
msgid "Use TCP (not UDP)"
msgstr "Използвай TCP (не UDP)"

msgctxt "w_advanced_proxy"
msgid "Proxy"
msgstr "Прокси"

msgctxt "w_advanced_proxy_desc"
msgid "Proxy domain/IP[:port] (optional, not always needed)"
msgstr "Прокси домейн/IP[:порт] (по избор, не винаги е необходимо)"

