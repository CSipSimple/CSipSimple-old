msgctxt "prefs"
msgid "Settings"
msgstr "Options"

msgctxt "prefs_fast"
msgid "Easy configuration"
msgstr "Configuration simplifiée"

msgctxt "prefs_fast_desc"
msgid "Quick options to use if you don't know how SIP works"
msgstr "Options rapides (pour ceux qui ne connaissent pas le SIP)"

msgctxt "prefs_network"
msgid "Network"
msgstr "Réseau"

msgctxt "prefs_network_desc"
msgid "How SIP can be connected to the network"
msgstr "La façon dont le SIP se connecte au réseau"

msgctxt "prefs_media"
msgid "Media"
msgstr "Média"

msgctxt "prefs_media_desc"
msgid "Codecs and in-call sound behavior"
msgstr "Comportement des codecs et du son en communication"

msgctxt "prefs_ui"
msgid "User interface"
msgstr "Apparence et comportement"

msgctxt "prefs_ui_desc"
msgid "Tweak the user interface"
msgstr "Modifier lapparence et les comportement de lapplication"

msgctxt "for_incomings"
msgid "For incoming calls"
msgstr "Pour les appels entrants"

msgctxt "for_outgoings"
msgid "For outgoing calls"
msgstr "Pour les appels sortants"

msgctxt "perfs"
msgid "Performances"
msgstr "Performances"

msgctxt "transport"
msgid "Transport"
msgstr "Transport"

msgctxt "use_wifi"
msgid "Use WiFi"
msgstr "Utiliser le WIFI"

msgctxt "use_wifi_in_desc"
msgid "Use WiFi for incoming calls"
msgstr "Utiliser le WIFI pour les appels entrants"

msgctxt "use_wifi_out_desc"
msgid "Use WiFi for outgoing calls"
msgstr "Utiliser le WIFI pour les appels sortants"

msgctxt "use_other"
msgid "Use other networks"
msgstr "Utiliser les autres réseaux"

msgctxt "use_other_in_desc"
msgid "For example WiMax for incoming calls"
msgstr "Par example le WiMax pour les appels entrants"

msgctxt "use_other_out_desc"
msgid "For example WiMax for outgoing calls"
msgstr "Par example le WiMax pour les appels sortant"

msgctxt "use_3g"
msgid "Use 3G"
msgstr "Utiliser la 3G"

msgctxt "use_3g_disclamer"
msgid "Your carrier MUST allow you to use this option"
msgstr "Votre opérateur DOIT vous lautoriser pour activer cette option"

msgctxt "use_edge"
msgid "Use EDGE"
msgstr "Utiliser le EDGE"

msgctxt "use_gprs"
msgid "Use GPRS"
msgstr "Utiliser le GPRS"

msgctxt "lock_wifi"
msgid "Lock WiFi"
msgstr "Verouiller le WIFI"

msgctxt "lock_wifi_desc"
msgid "Prevent WiFi idle when SIP is connected"
msgstr "Empêche la mise en veille du WIFI lorsque le SIP est connecté"

msgctxt "enable_tcp"
msgid "TCP"
msgstr "TCP"

msgctxt "enable_tcp_desc"
msgid "Enable TCP transport"
msgstr "Autoriser le transport TCP"

msgctxt "enable_tls"
msgid "TLS"
msgstr "TLS"

msgctxt "enable_tls_desc"
msgid "Enable TLS transport"
msgstr "Autoriser le transport TLS"

msgctxt "enable_udp"
msgid "UDP"
msgstr "UDP"

msgctxt "enable_udp_desc"
msgid "Enable UDP transport"
msgstr "Autoriser le transport UDP"

msgctxt "network_udp_transport_port"
msgid "UDP Port"
msgstr "Port UDP"

msgctxt "network_udp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_tcp_transport_port"
msgid "TCP Port"
msgstr "Port TCP"

msgctxt "network_tcp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_tls_transport_port"
msgid "TLS Port"
msgstr "Port TLS"

msgctxt "network_tls_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_rtp_port"
msgid "RTP port"
msgstr "Port RTP"

msgctxt "network_rtp_port_desc"
msgid "Port for RTP ports range start"
msgstr "Port pour le début de la fourchette de port RTP"

msgctxt "use_ipv6"
msgid "Use IPv6"
msgstr "Utiliser l'IPv6"

msgctxt "use_ipv6_desc"
msgid "Switch to IPv6 mode (your network and phone must support it)"
msgstr ""
"Passer en mode IPv6 (votre téléphone et votre réseau doivent le supporter)"

msgctxt "enable_dns_srv"
msgid "Resolve DNS SRV"
msgstr "Résoudre les DNS SRV"

msgctxt "enable_dns_srv_desc"
msgid "Use CSipSimple nameserver resolver resolve names"
msgstr "Utiliser les DNS dans CSipSimple pour résoudre les noms de domaine"

msgctxt "override_nameserver"
msgid "Use a custom DNS"
msgstr "DNS personnalisé"

msgctxt "override_nameserver_desc"
msgid "Left blank to retrieve DNS from Android"
msgstr "Laisser vide pour récupérer les DNS d'Android"

msgctxt "audio_quality"
msgid "Audio quality"
msgstr "Qualité audio"

msgctxt "codecs"
msgid "Codecs"
msgstr "Codecs"

msgctxt "ice_turn"
msgid "ICE/TURN/STUN"
msgstr "ICE/TURN"

msgctxt "echo_cancellation"
msgid "Echo cancellation"
msgstr "Annulation de l'echo"

msgctxt "echo_cancellation_desc"
msgid "Activate echo cancellation"
msgstr "Activer l'annulation d'echo"

msgctxt "enable_vad"
msgid "Voice audio detection"
msgstr "Détection de silence"

msgctxt "enable_vad_desc"
msgid "Activate voice audio detection"
msgstr ""
"Activer la détection active de voix (réduit la bande passante utilisée)"

msgctxt "snd_mic_level"
msgid "Micro amplification"
msgstr "Amplification du micro"

msgctxt "snd_mic_level_desc"
msgid "Adjust the micro signal level (1 do nothing)"
msgstr "Ajuster le gain du signal du micro (1 n'a pas d'effet)"

msgctxt "snd_speaker_level"
msgid "Speaker amplification"
msgstr "Amplification de l'écoute"

msgctxt "snd_speaker_level_desc"
msgid "Adjust the speaker signal level (1 do nothing)"
msgstr "Ajuster le gain du signal de l'écouteur (1 n'a pas d'effet)"

msgctxt "snd_stream_level"
msgid "Initial stream level"
msgstr "Niveau sonore initial"

msgctxt "snd_stream_level_desc"
msgid "Set the initial level for audio stream (Android stream)"
msgstr "Choix du niveau initial du flux sonore Android"

msgctxt "snd_auto_close_time"
msgid "Auto close time"
msgstr "Fermeture automatique du canal audio"

msgctxt "snd_auto_close_time_desc"
msgid "Time to wait for close audio channel"
msgstr ""
"Temps avant fermture du canal audio (peut dans certain cas régler le "
"problème de fermeture du canal audio sur android 1.5)"

msgctxt "snd_media_quality"
msgid "Media quality"
msgstr "Qualité du média"

msgctxt "snd_media_quality_desc"
msgid ""
"Quality of the media stream (modify only if you know what you are doing)"
msgstr ""
"Qualité du flux média (à ne modifier que si vous savez exactement ce que "
"vous faites)"

msgctxt "snd_clock_rate"
msgid "Clock rate"
msgstr "Fréquence"

msgctxt "snd_clock_rate_desc"
msgid "Audio channel frequency"
msgstr "Fréquence du canal audio"

msgctxt "snd_ptime"
msgid "Frame ptime"
msgstr "Ptime de la frame"

msgctxt "snd_ptime_desc"
msgid "Specifying lower ptime will normally reduce the latency"
msgstr "Un ptime plus petit réduit la latence"

msgctxt "echo_cancellation_tail"
msgid "Echo cancellation tail"
msgstr "Longueur d'annulation de l'echo"

msgctxt "echo_cancellation_tail_desc"
msgid "Tail length of echo cancellation"
msgstr "Longueur de la trainée sur laquelle appliquer l'annulation d'écho"

msgctxt "enable_ice"
msgid "Enable ICE"
msgstr "Activer ICE"

msgctxt "enable_ice_desc"
msgid "Turn on ICE feature"
msgstr "Activer la fonctionnalité ICE"

msgctxt "enable_turn"
msgid "Enable TURN"
msgstr "Activer TURN"

msgctxt "enable_turn_desc"
msgid "Turn on TURN feature"
msgstr "Activer la fonctionnalité TURN"

msgctxt "turn_server"
msgid "Turn server"
msgstr "Serveur TURN"

msgctxt "turn_server_desc"
msgid "TURN server (HOST:PORT)"
msgstr "Serveur TURN (HOST:PORT)"

msgctxt "enable_stun"
msgid "Enable STUN"
msgstr "Activer STUN"

msgctxt "enable_stun_desc"
msgid "Turn on STUN feature"
msgstr "Activer la foncionnalité STUN"

msgctxt "stun_server"
msgid "STUN server"
msgstr "Serveur STUN"

msgctxt "stun_server_desc"
msgid "STUN server (HOST:PORT)"
msgstr "Server STUN (hôte:port)"

msgctxt "codecs_desc"
msgid "Priority (0 is disabled)"
msgstr "Priorité (0 = désactivé)"

msgctxt "use_srtp"
msgid "SRTP mode"
msgstr "Mode SRTP"

msgctxt "use_srtp_desc"
msgid "Mode for Secure RTP"
msgstr "Mode pour le RTP sécurisé"

msgctxt "general_ui"
msgid "General user interface"
msgstr "Interface utilisateur générale"

msgctxt "native_dialer_integration"
msgid "Native dialer integration"
msgstr "Intégration au numéroteur Android"

msgctxt "advanced_ui"
msgid "Advanced tweaks"
msgstr "Modifications avancées"

msgctxt "integrate_with_native_dialer"
msgid "Dialer integration"
msgstr "Activer l'intégration"

msgctxt "integrate_with_native_dialer_desc"
msgid ""
"When dialing with native dialer you'll be asked if you want to use SIP or "
"mobile"
msgstr ""
"Quand un appel est initié partout dans Android il vous sera demandé si vous "
"voulez utiliser le GSM ou le SIP"

msgctxt "integrate_with_native_calllogs"
msgid "Call logs integration"
msgstr "Intégrations au journal d'appel"

msgctxt "integrate_with_native_calllogs_desc"
msgid "SIP calls will be merged (if possible) into native call logs"
msgstr ""
"Les appels SIP seront rajoutés (si possible) aux journaux d'appels d'Android"

msgctxt "exclude_for_outgoing_call"
msgid "Exclude patterns"
msgstr "Motifs à exclures"

msgctxt "exclude_for_outgoing_call_desc"
msgid "Exclude patterns for outgoing calls"
msgstr "Motifs (regexp) pour les appels sortant à ne pas traiter en SIP"

msgctxt "dial_press_tone"
msgid "Dialing tone feedback"
msgstr "Retour sonore du numéroteur"

msgctxt "dial_press_tone_desc"
msgid "Play touch tones when dialing"
msgstr "Jouer un son à l'appui sur un numéro"

msgctxt "dial_press_vibrate"
msgid "Dialing vibrate feedback"
msgstr "Retour vibration du numéroteur"

msgctxt "dial_press_vibrate_desc"
msgid "Haptic (vibrate) feedback when dialing"
msgstr "Vibrer lors de l'appui sur un numéro"

msgctxt "start_with_text_dialer"
msgid "Text dialer"
msgstr "Numérotation texte"

msgctxt "start_with_text_dialer_desc"
msgid "Start with text dialer"
msgstr "Démarrer avec le numéroteur de type texte"

msgctxt "use_alternate_unlocker"
msgid "Don't use slide unlocker"
msgstr "Dévérouillage bouttons"

msgctxt "use_alternate_unlocker_desc"
msgid "Use buttons instead of slide to take incoming calls"
msgstr "Ne pas utiliser le dévérouillage glissant pour prendre les appels"

msgctxt "keep_awake_incall"
msgid "Keep awake while on call"
msgstr "Rester allumer lorsqu'en cours d'appel"

msgctxt "keep_awake_incall_desc"
msgid "Fixes if you get choppy sound when screen turn off while in WIFI call"
msgstr ""
"Corrige le son haché lorsque le téléphone est en WIFI (par exemple pour le "
"Nexus One)"

msgctxt "use_partial_wake_lock"
msgid "Use partial wake lock"
msgstr "Utiliser le verouillage CPU"

msgctxt "use_partial_wake_lock_desc"
msgid "Ensure CPU is always running for CSipSimple (drain battery)"
msgstr ""
"S'assure que le CPU est toujours actif pour CSipSimple (use de la batterie)"

msgctxt "log_level"
msgid "Log level"
msgstr "Niveau de traces"

msgctxt "log_level_desc"
msgid "Log level for debug between 0 (none) and 5 (verbose)"
msgstr "Niveau des traces produites entre 0 (aucune) et 5 (verbeux)"

msgctxt "integrate_with_native_music"
msgid "Integrate with Music application"
msgstr "S'intégrer à l'application musique"

msgctxt "integrate_with_native_music_desc"
msgid "Disable it if you use another music application than the official one"
msgstr ""
"A désactiver si vous utilisez un autre application que l'application "
"officielle"

msgctxt "android_integration"
msgid "Integrate with Android"
msgstr "S'intégrer à Android"

msgctxt "android_integration_desc"
msgid "Integration with native dialer and call logs"
msgstr "S'intégrer à la numérotation et aux journaux d'appel d'Android"

msgctxt "availability_profile"
msgid "Availability profile"
msgstr "Profil de disponibilité"

msgctxt "always_available"
msgid "Always available"
msgstr "Toujours disponible"

msgctxt "always_available_desc"
msgid "Will use battery but incoming calls are always received"
msgstr "Utilisera de la batterie mais vous recevrai vos appels"

msgctxt "wifi_available"
msgid "Available on WiFi"
msgstr "Disponible en même temps que le WIFI"

msgctxt "wifi_available_desc"
msgid "Availability of incoming calls based on WiFi profile"
msgstr "Votre disponibilité est basée la connexion Wifi"

msgctxt "never_available"
msgid "Only for outgoing"
msgstr "Seulement pour les appels sortant"

msgctxt "never_available_desc"
msgid "Activate the SIP client automatically when necessary"
msgstr "N'activer CSipSimple que lorsque j'initie un appel."

msgctxt "availability_tg"
msgid "Mobile data availability"
msgstr "Disponibilité du canal donnée mobile"

msgctxt "tg_allowed"
msgid "I'm allowed to use mobile"
msgstr "Je suis autorisé à faire de la VoIP sur le canal donnée du mobile"

msgctxt "tg_allowed_desc"
msgid "Your carrier allows VoIP over your mobile data connection"
msgstr ""
"Votre opérateur vous autorise à faire de la VoIP sur la canal data de sa "
"connection"

msgctxt "codecs_availability_text"
msgid "Only available if sip is currently registered"
msgstr "Disponible seulement si le SIP est actuellement enregistré"
