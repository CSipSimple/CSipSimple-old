msgctxt "prefs"
msgid "Settings"
msgstr "Options"

msgctxt "prefs_fast"
msgid "Easy configuration"
msgstr "Configuration simplifiée"

msgctxt "prefs_fast_desc"
msgid "Quick options to use if you don't know how SIP works"
msgstr "Options rapides (pour ceux qui ne connaissent pas le SIP)"

msgctxt "prefs_network"
msgid "Network"
msgstr "Réseau"

msgctxt "prefs_network_desc"
msgid "How SIP can be connected to the network"
msgstr "La façon dont le SIP se connecte au réseau"

msgctxt "prefs_media"
msgid "Media"
msgstr "Média"

msgctxt "prefs_media_desc"
msgid "Codecs and in-call sound behavior"
msgstr "Règles média pendant les appels (Codecs, Silences, ...)"

msgctxt "prefs_ui"
msgid "User interface"
msgstr "Interface et intégration"

msgctxt "prefs_ui_desc"
msgid "Tweak the user interface"
msgstr "Adaptez le comportement à votre usage"

msgctxt "prefs_calls"
msgid "Calls options"
msgstr "Options d'appel"

msgctxt "prefs_calls_desc"
msgid "Options related to calls"
msgstr "Options relatives aux appels"

#. Network
msgctxt "for_incomings"
msgid "For incoming calls"
msgstr "Pour les appels entrants"

msgctxt "for_outgoings"
msgid "For outgoing calls"
msgstr "Pour les appels sortants"

msgctxt "perfs"
msgid "Performances"
msgstr "Performances"

msgctxt "transport"
msgid "Transport"
msgstr "Transport"

msgctxt "transport_desc"
msgid "Force the use of a transport for this account"
msgstr "Forcer le protocole de communication"

msgctxt "use_wifi"
msgid "Use WiFi"
msgstr "Utiliser le WIFI"

msgctxt "use_wifi_in_desc"
msgid "Use WiFi for incoming calls"
msgstr "Utiliser le WIFI pour les appels entrants"

msgctxt "use_wifi_out_desc"
msgid "Use WiFi for outgoing calls"
msgstr "Utiliser le WIFI pour les appels sortants"

msgctxt "use_other"
msgid "Use other networks"
msgstr "Utiliser les autres réseaux"

msgctxt "use_other_in_desc"
msgid "For example WiMax for incoming calls"
msgstr "Par example le WiMax pour les appels entrants"

msgctxt "use_other_out_desc"
msgid "For example WiMax for outgoing calls"
msgstr "Par example le WiMax pour les appels sortant"

msgctxt "use_3g"
msgid "Use 3G"
msgstr "Utiliser la 3G"

msgctxt "use_3g_disclamer"
msgid "Your carrier MUST allow you to use this option"
msgstr "Votre opérateur DOIT vous autoriser l'utilisation cette option"

msgctxt "use_edge"
msgid "Use EDGE"
msgstr "Utiliser le EDGE"

msgctxt "use_gprs"
msgid "Use GPRS"
msgstr "Utiliser le GPRS"

msgctxt "lock_wifi"
msgid "Lock WiFi"
msgstr "Verouiller le WIFI"

msgctxt "lock_wifi_desc"
msgid "Prevent WiFi idle when SIP is connected"
msgstr "Empêche la mise en veille du WIFI lorsque le SIP est connecté"

msgctxt "lock_wifi_perfs"
msgid "High perfs lock"
msgstr "Verrou haute performance"

msgctxt "lock_wifi_perfs_desc"
msgid "Wifi lock in High perfs mode (drain battery faster)"
msgstr "Verrou wifi en mode haute performance (utilise plus la batterie)"

msgctxt "enable_tcp"
msgid "TCP"
msgstr "TCP"

msgctxt "enable_tcp_desc"
msgid "Enable TCP transport"
msgstr "Autoriser le transport TCP"

msgctxt "enable_tls"
msgid "TLS"
msgstr "TLS"

msgctxt "enable_tls_desc"
msgid "Enable TLS transport"
msgstr "Autoriser le transport TLS"

msgctxt "enable_udp"
msgid "UDP"
msgstr "UDP"

msgctxt "enable_udp_desc"
msgid "Enable UDP transport"
msgstr "Autoriser le transport UDP"

msgctxt "network_udp_transport_port"
msgid "UDP Port"
msgstr "Port UDP"

msgctxt "network_udp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_tcp_transport_port"
msgid "TCP Port"
msgstr "Port TCP"

msgctxt "network_tcp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_rtp_port"
msgid "RTP port"
msgstr "Port RTP"

msgctxt "network_rtp_port_desc"
msgid "Port for RTP ports range start"
msgstr "Port pour le début de la fourchette de port RTP"

msgctxt "use_ipv6"
msgid "Use IPv6"
msgstr "Utiliser l'IPv6"

msgctxt "use_ipv6_desc"
msgid "Switch to IPv6 mode (your network and phone must support it)"
msgstr ""
"Passer en mode IPv6 (votre téléphone et votre réseau doivent le supporter)"

msgctxt "enable_dns_srv"
msgid "Resolve DNS SRV"
msgstr "Résoudre les DNS SRV"

msgctxt "enable_dns_srv_desc"
msgid "Use the application resolver to resolve DNS names"
msgstr ""
"Utiliser les DNS dans l'application pour résoudre les noms de domaine"

msgctxt "override_nameserver"
msgid "Use a custom DNS"
msgstr "DNS personnalisé"

msgctxt "override_nameserver_desc"
msgid "Left blank to retrieve DNS from Android"
msgstr "Laisser vide pour récupérer les DNS d'Android"

msgctxt "nat_traversal"
msgid "NAT traversal"
msgstr "Traversée du NAT"

msgctxt "enable_ice"
msgid "Enable ICE"
msgstr "Activer ICE"

msgctxt "enable_ice_desc"
msgid "Turn on ICE feature"
msgstr "Activer la fonctionnalité ICE"

msgctxt "enable_turn"
msgid "Enable TURN"
msgstr "Activer TURN"

msgctxt "enable_turn_desc"
msgid "Turn on TURN feature"
msgstr "Activer la fonctionnalité TURN"

msgctxt "turn_server"
msgid "Turn server"
msgstr "Serveur TURN"

msgctxt "turn_server_desc"
msgid "TURN server (HOST:PORT)"
msgstr "Serveur TURN (HOST:PORT)"

msgctxt "turn_username"
msgid "TURN user"
msgstr "Utilisateur TURN"

msgctxt "turn_username_desc"
msgid "Username for TURN authentication"
msgstr "Nom d'utilisateur pour l'authentification TURN"

msgctxt "turn_password"
msgid "TURN password"
msgstr "Mot de passe TURN"

msgctxt "turn_password_desc"
msgid "Password for TURN authentication"
msgstr "Mot de passe pour l'authentification TURN"

msgctxt "enable_stun"
msgid "Enable STUN"
msgstr "Activer STUN"

msgctxt "enable_stun_desc"
msgid "Turn on STUN feature"
msgstr "Activer la fonctionnalité STUN"

msgctxt "stun_server"
msgid "STUN servers"
msgstr "Serveur STUN"

msgctxt "stun_server_desc"
msgid "STUN servers (HOST[:PORT], ...)"
msgstr "Serveur STUN (hôte[:port],...)"

msgctxt "enable_qos"
msgid "Enable QoS"
msgstr "Activer la QoS"

msgctxt "enable_qos_desc"
msgid "Enable QoS on packets"
msgstr "Activer la QoS sur les paquets"

msgctxt "dscp_val"
msgid "DSCP value"
msgstr "Valeur DSCP"

msgctxt "dscp_val_desc"
msgid "DSCP value for SIP traffic"
msgstr "Valeur DSCP pour le trafic SIP"

msgctxt "keep_alive_interval_wifi"
msgid "Wifi keep alive"
msgstr "Battement de coeur wifi"

msgctxt "keep_alive_interval_mobile"
msgid "Mobile keep alive"
msgstr "Battement de coeur mobile"

msgctxt "keep_alive_interval_desc"
msgid "Interval (sec) for KA packets"
msgstr "Intervalle (seconde) pour les paquets qui garde la connexion active"

msgctxt "use_compact_form"
msgid "Use compact SIP"
msgstr "Utiliser le SIP compact"

msgctxt "use_compact_form_desc"
msgid "Reduce SIP packets size. May not work on old SIP servers"
msgstr ""
"Réduit la taille des paquets SIP. Peut ne pas être supporté par certains "
"fournisseurs SIP"

msgctxt "user_agent"
msgid "User agent"
msgstr "Agent utilisateur"

msgctxt "user_agent_desc"
msgid "Agent presented by the application (if default will add infos)"
msgstr ""
"Agent présenté par l'application (si défaut, ajoutera des infos plateforme)"

msgctxt "ka_use_wake"
msgid "Keep alive awake"
msgstr "Reveil du battement de coeur"

msgctxt "ka_use_wake_desc"
msgid "If activated keep alive will awake phone"
msgstr "Si actif, le battement de coeur réveille le téléphone"

#. Security
msgctxt "tls"
msgid "TLS"
msgstr "Chiffrement TLS"

msgctxt "secure_transport"
msgid "Secure transport"
msgstr "Transport sécurisé"

msgctxt "secure_transport_desc"
msgid "Security mechanism for SIP and RTP"
msgstr "Mécanismes de sécurité pour le SIP et RTP"

msgctxt "secure_media"
msgid "Secure media"
msgstr "Média sécurisé"

msgctxt "network_tls_transport_port"
msgid "TLS Port"
msgstr "Port TLS"

msgctxt "network_tls_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numéro du port sur lequel écouter localement"

msgctxt "network_tls_server_name"
msgid "TLS server name"
msgstr "Nom du serveur TLS"

msgctxt "network_tls_server_name_desc"
msgid "For multi-hosting server (optional)"
msgstr "Pour des serveurs mutualisés (optionnel)"

msgctxt "ca_list_file"
msgid "TLS CA file"
msgstr "Fichier TLS CA (certificat)"

msgctxt "ca_list_file_desc"
msgid "For example /sdcard/CSipSimple/myAuthority.ca"
msgstr "Par exemple /sdcard/CSipSimple/myAuthority.ca"

msgctxt "cert_file"
msgid "TLS certificate file"
msgstr "Certificat TLS"

msgctxt "cert_file_desc"
msgid "For example /sdcard/CSipSimple/myCert.crt"
msgstr "Par exemple /sdcard/CSipSimple/myCert.crt"

msgctxt "privkey_file"
msgid "TLS private key file"
msgstr "Fichier de clef privée TLS"

msgctxt "privkey_file_desc"
msgid "For example /sdcard/CSipSimple/myKey.key"
msgstr "Par exemple /sdcard/CSipSimple/myKey.key"

msgctxt "tls_password"
msgid "TLS password"
msgstr "Mot de passe TLS"

msgctxt "tls_password_desc"
msgid "TLS password to private key file"
msgstr "Mot de passe de la clef privée TLS"

msgctxt "verify_server"
msgid "Check server"
msgstr "Vérifier le serveur"

msgctxt "verify_server_desc"
msgid "Verify server's certificate"
msgstr "Vérifier le certificat du serveur"

msgctxt "verify_client"
msgid "Check client"
msgstr "Vérifier le client"

msgctxt "verify_client_desc"
msgid "Verify client's certificate"
msgstr "vérifier le certificat du client"

msgctxt "tls_method"
msgid "TLS Method"
msgstr "Méthode TLS"

msgctxt "tls_method_desc"
msgid "Method for TLS transport"
msgstr "Méthode de transport TLS"

msgctxt "use_srtp"
msgid "SRTP mode"
msgstr "Mode SRTP"

msgctxt "use_srtp_desc"
msgid "Mode for Secure RTP"
msgstr "Mode pour le RTP sécurisé"

msgctxt "use_zrtp"
msgid "ZRTP mode"
msgstr "Mode ZRTP"

msgctxt "use_zrtp_desc"
msgid "Mode for Secured ZRTP"
msgstr "Mode pour la sécurité ZRTP"

#. Media
msgctxt "audio_quality"
msgid "Audio quality"
msgstr "Qualité audio"

msgctxt "audio_volume"
msgid "Volume"
msgstr "Volume"

msgctxt "codecs"
msgid "Codecs"
msgstr "Codecs"

msgctxt "echo_cancellation"
msgid "Echo cancellation"
msgstr "Annulation de l'echo"

msgctxt "echo_cancellation_desc"
msgid "Activate echo cancellation"
msgstr "Activer l'annulation d'echo"

msgctxt "enable_vad"
msgid "Voice audio detection"
msgstr "Détection de silence"

msgctxt "enable_vad_desc"
msgid "Activate voice audio detection"
msgstr ""
"Activer la détection active de voix (réduit la bande passante utilisée)"

msgctxt "snd_mic_level"
msgid "Micro amplification"
msgstr "Amplification du micro"

msgctxt "snd_mic_level_desc"
msgid "Adjust the micro signal level (1 do nothing)"
msgstr "Ajuster le gain du signal du micro (1 n'a pas d'effet)"

msgctxt "snd_speaker_level"
msgid "Speaker amplification"
msgstr "Amplification de l'écoute"

msgctxt "snd_speaker_level_desc"
msgid "Adjust the speaker signal level (1 do nothing)"
msgstr "Ajuster le gain du signal de l'écouteur (1 n'a pas d'effet)"

msgctxt "snd_bt_mic_level"
msgid "Bluetooth micro amplification"
msgstr "Amplification du micro Bluetooth"

msgctxt "snd_bt_speaker_level"
msgid "Bluetooth listening amplification"
msgstr "Amplification d'écoute Bluetooth"

msgctxt "snd_stream_level"
msgid "Initial stream level"
msgstr "Niveau sonore initial"

msgctxt "snd_stream_level_desc"
msgid "Set the initial level for audio stream (Android stream)"
msgstr "Choix du niveau initial du flux sonore Android"

msgctxt "snd_auto_close_time"
msgid "Auto close time"
msgstr "Fermeture automatique du canal audio"

msgctxt "snd_auto_close_time_desc"
msgid "Time to wait for close audio channel"
msgstr ""
"Temps avant fermture du canal audio (peut dans certain cas régler le "
"problème de fermeture du canal audio sur android 1.5)"

msgctxt "snd_media_quality"
msgid "Media quality"
msgstr "Qualité du média"

msgctxt "snd_media_quality_desc"
msgid ""
"Quality of the media stream (modify only if you know what you are doing)"
msgstr ""
"Qualité du flux média (à ne modifier que si vous savez exactement ce que "
"vous faites)"

msgctxt "snd_clock_rate"
msgid "Clock rate"
msgstr "Fréquence"

msgctxt "snd_clock_rate_desc"
msgid "Audio channel frequency"
msgstr "Fréquence du canal audio"

msgctxt "snd_ptime"
msgid "Frame ptime"
msgstr "Ptime de la frame"

msgctxt "snd_ptime_desc"
msgid "Specifying lower ptime will normally reduce the latency"
msgstr "Un ptime plus petit réduit la latence"

msgctxt "echo_cancellation_tail"
msgid "Echo cancellation tail"
msgstr "Longueur d'annulation de l'écho"

msgctxt "echo_cancellation_tail_desc"
msgid "Tail length of echo cancellation"
msgstr "Longueur de la trainée sur laquelle appliquer l'annulation d'écho"

msgctxt "echo_mode"
msgid "Echo mode"
msgstr "Mode de l'écho"

msgctxt "echo_mode_desc"
msgid "Algorithm for echo"
msgstr "Algorithme pour réduire l'écho"

msgctxt "codecs_desc"
msgid "Priority (0 is disabled)"
msgstr "Priorité (0 = désactivé)"

msgctxt "sip_dtmf"
msgid "DTMF mode"
msgstr "Modes DTMF"

msgctxt "sip_dtmf_desc"
msgid "How to send DTMF"
msgstr "Manière d'envoyer les signaux DTMF"

msgctxt "use_soft_volume"
msgid "Use software volume"
msgstr "Utiliser le volume logiciel"

msgctxt "use_soft_volume_desc"
msgid "Use software volume instead of android volume"
msgstr "Utiliser le volume logiciel au lieu du volume Android"

msgctxt "use_routing_api"
msgid "Use routing API"
msgstr "Utiliser l'API de routing"

msgctxt "use_routing_api_desc"
msgid "Try to enable that if audio is routing to speaker instead of earpiece"
msgstr ""
"Essayer d'activer cela si le son est sur le haut parleur au lieu de "
"l'écouteur"

msgctxt "use_mode_api"
msgid "Use Mode audio API"
msgstr "Utiliser le Mode Audio API"

msgctxt "use_mode_api_desc"
msgid "Try to enable that if audio is routing to speaker instead of earpiece"
msgstr ""
"Essayez ce mode si le flux audio passe par le haut parleur en mode mains "
"libres"

msgctxt "sip_audio_mode"
msgid "Audio mode for SIP calls"
msgstr "Mode audio des appels SIP"

msgctxt "sip_audio_mode_desc"
msgid "Change only if developers advise you to"
msgstr "Ne changer ceci que si les développeurs vous le conseillent"

msgctxt "use_webrtc_hack"
msgid "Use WebRTC implementation"
msgstr "Utiliser l'implémentation WebRTC"

msgctxt "use_webrtc_hack_desc"
msgid "Implementation made by WebRTC team"
msgstr "Implémentation faite par l'équipe WebRTC"

msgctxt "do_focus_audio"
msgid "Focus audio"
msgstr "Acquisition audio"

msgctxt "do_focus_audio_desc"
msgid "Disabling will result overlaps with music but may help with routing"
msgstr ""
"Désactiver conduira le son de la musique et la communication a rentrer en "
"conflit mais peut aider le routage."

msgctxt "micro_source"
msgid "Micro source"
msgstr "Micro source"

msgctxt "micro_source_desc"
msgid ""
"Source for micro. Choosing the appropriate value may improve echo "
"cancellation"
msgstr ""
"Source pour le micro. Choisir la source approprié peut grandement améliorer "
"l'annulation d'écho"

msgctxt "has_io_queue"
msgid "IO Queue"
msgstr "File d'entrée / sortie"

msgctxt "has_io_queue_desc"
msgid "Activate Queue for input/output"
msgstr "Activer la file entrée / sortie"

msgctxt "set_audio_generate_tone"
msgid "Tone hack"
msgstr "Hack de la tonalité"

msgctxt "set_audio_generate_tone_desc"
msgid "Generate a tone before making calls"
msgstr "Générer une tonalité avant d'effectuer un appel"

msgctxt "narrow_band"
msgid "Narrowband"
msgstr "Bas débit"

msgctxt "wide_band"
msgid "Wideband"
msgstr "Haut débit"

msgctxt "band_for_wifi"
msgid "Band type for wifi"
msgstr "Type de débit pour le wifi"

msgctxt "band_for_3g"
msgid "Band type for 3G"
msgstr "Type de débit pour 3G"

msgctxt "band_for_gprs"
msgid "Band type for GPRS"
msgstr "Type de débit pour GPRS"

msgctxt "band_for_edge"
msgid "Band type for EDGE"
msgstr "Type de débit pour EDGE"

msgctxt "band_for_other"
msgid "Band type for other"
msgstr "Type de débit pour les autres"

msgctxt "codecs_band_types"
msgid "Connections bandwidth types for codecs"
msgstr "Type de connections pour les codecs"

msgctxt "use_sgs_call_hack"
msgid "Use Samsung Galaxy Hack"
msgstr "Utiliser le Hack pour Samsung Galaxy"

msgctxt "use_sgs_call_hack_desc"
msgid "Pre set call mode to solve routing"
msgstr "Pré configure le mode pour résoudre le problème de routage audio"

#. UI
msgctxt "general_ui"
msgid "General user interface"
msgstr "Interface utilisateur générale"

msgctxt "native_dialer_integration"
msgid "Native dialer integration"
msgstr "Intégration au numéroteur Android"

msgctxt "advanced_ui"
msgid "Advanced tweaks"
msgstr "Modifications avancées"

msgctxt "integrate_with_native_dialer"
msgid "Dialer integration"
msgstr "Activer l'intégration"

msgctxt "integrate_with_native_dialer_desc"
msgid ""
"When dialing with native dialer you'll be asked if you want to use SIP or "
"mobile"
msgstr ""
"Quand un appel est initié partout dans Android il vous sera demandé si vous "
"voulez utiliser le GSM ou le SIP"

msgctxt "integrate_with_native_calllogs"
msgid "Call logs integration"
msgstr "Intégrations au journal d'appel"

msgctxt "integrate_with_native_calllogs_desc"
msgid "SIP calls will be merged (if possible) into native call logs"
msgstr ""
"Les appels SIP seront rajoutés (si possible) aux journaux d'appels d'Android"

msgctxt "dial_press_tone"
msgid "Dialing tone feedback"
msgstr "Retour sonore du numéroteur"

msgctxt "dial_press_tone_desc"
msgid "Play touch tones when dialing"
msgstr "Jouer un son à l'appui sur un numéro"

msgctxt "dial_press_vibrate"
msgid "Dialing vibrate feedback"
msgstr "Retour vibration du numéroteur"

msgctxt "dial_press_vibrate_desc"
msgid "Haptic (vibrate) feedback when dialing"
msgstr "Vibrer lors de l'appui sur un numéro"

msgctxt "start_with_text_dialer"
msgid "Text dialer"
msgstr "Numérotation texte"

msgctxt "start_with_text_dialer_desc"
msgid "Start with text dialer"
msgstr "Démarrer avec le numéroteur de type texte"

msgctxt "use_alternate_unlocker"
msgid "Don't use slide answer"
msgstr "Ne pas utiliser le Slide pour répondre"

msgctxt "use_alternate_unlocker_desc"
msgid "Use buttons instead of slide to take incoming calls"
msgstr "Ne pas utiliser le dévérouillage glissant pour prendre les appels"

msgctxt "keep_awake_incall"
msgid "Keep awake while on call"
msgstr "Rester allumer lorsqu'en cours d'appel"

msgctxt "keep_awake_incall_desc"
msgid "Fixes if you get choppy sound when screen turn off while in WIFI call"
msgstr ""
"Corrige le son haché lorsque le téléphone est en WIFI (par exemple pour le "
"Nexus One)"

msgctxt "use_partial_wake_lock"
msgid "Use partial wake lock"
msgstr "Utiliser le verouillage CPU"

msgctxt "use_partial_wake_lock_desc"
msgid "Ensure CPU is always running for the application (drain battery)"
msgstr ""
"S'assurer que le CPU tourne toujours pour l'application (utilise la batterie)"

msgctxt "log_level"
msgid "Log level"
msgstr "Niveau de traces"

msgctxt "log_level_desc"
msgid "Log level for debug between 0 (none) and 5 (verbose)"
msgstr "Niveau des traces produites entre 0 (aucune) et 5 (verbeux)"

msgctxt "integrate_with_native_music"
msgid "Integrate with Music application"
msgstr "S'intégrer à Musique"

msgctxt "integrate_with_native_music_desc"
msgid "Disable it if you use another music application than the official one"
msgstr ""
"A désactiver si vous utilisez un autre application que l'application "
"officielle"

msgctxt "ringtone"
msgid "Ringtone"
msgstr "Sonnerie"

msgctxt "ringtone_desc"
msgid "Default ringtone for SIP calls"
msgstr "Sonnerie à défaut pour les appels SIP"

msgctxt "prevent_screen_rotation"
msgid "Prevent screen rotation"
msgstr "Empêcher la rotation d'écran"

msgctxt "prevent_screen_rotation_desc"
msgid "Avoid rotation of the screen and keep in portrait mode"
msgstr "Eviter la rotation de l'écran et rester en mode portrait"

msgctxt "gsm_integration_type"
msgid "Mobile phone integration"
msgstr "Intégration au mobile"

msgctxt "gsm_integration_type_desc"
msgid "How the application uses your mobile phone (or not)"
msgstr "Comment l'application utilise votre ligne mobile (ou pas)"

msgctxt "invert_proximity_sensor"
msgid "Invert proximity sensor"
msgstr "Inverser le capteur de proximité"

msgctxt "invert_proximity_sensor_desc"
msgid "May help on some devices when manufacturer inverted near and far"
msgstr ""
"Peut résoudre les problèmes liés à une mauvaise implémentation du "
"constructeur"

msgctxt "theme"
msgid "Theme"
msgstr "Thème"

msgctxt "theme_desc"
msgid "Theme for the application"
msgstr "Thème pour l'application"

msgctxt "obtain_themes"
msgid "Obtain more themes"
msgstr "Obtenir plus de thèmes"

msgctxt "obtain_themes_desc"
msgid "Get themes apps on the market"
msgstr "Récupérer les applications thèmes depuis le market"

msgctxt "display_icon_in_status_bar_nbr"
msgid "Display number in status bar"
msgstr "Afficher le nombre dans la barre de status"

msgctxt "display_icon_in_status_bar_nbr_desc"
msgid "Display the number of registered accounts in status bar"
msgstr "Nombre de comptes enregistrés dans la barre de notification"

#. Fast settings
msgctxt "android_integration"
msgid "Integrate with Android"
msgstr "S'intégrer à Android"

msgctxt "android_integration_desc"
msgid "Integration with native dialer and call logs"
msgstr "S'intégrer à la numérotation et aux journaux d'appel d'Android"

msgctxt "availability_profile"
msgid "Availability profile"
msgstr "Profil de disponibilité"

msgctxt "always_available"
msgid "Always available"
msgstr "Toujours disponible"

msgctxt "always_available_desc"
msgid "Will use battery but incoming calls are always received"
msgstr "Utilisera de la batterie mais vous recevrez vos appels"

msgctxt "wifi_available"
msgid "Available on WiFi"
msgstr "Disponible en même temps que le WIFI"

