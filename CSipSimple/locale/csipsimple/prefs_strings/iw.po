msgctxt "advanced_ui"
msgid "Advanced tweaks"
msgstr "התאמה מתקדמת"

msgctxt "integrate_with_native_dialer"
msgid "Dialer integration"
msgstr "חייגן משולב"

msgctxt "integrate_with_native_dialer_desc"
msgid ""
"When dialing with native dialer you'll be asked if you want to use SIP or"
" mobile"
msgstr "כאשר מחייגים עם החייגן המקורי תישאל אם ברצונך להשתמש בסיפ או סלולרי"

msgctxt "integrate_with_native_calllogs"
msgid "Call logs integration"
msgstr "יומן שיחות משולב"

msgctxt "integrate_with_native_calllogs_desc"
msgid "SIP calls will be merged (if possible) into native call logs"
msgstr "שיחות סיפ ימוזגו (במידת האפשר) לתוך יומן השיחות המקורי"

msgctxt "dial_press_tone"
msgid "Dialing tone feedback"
msgstr "חיוג צליל תגובה"

msgctxt "dial_press_tone_desc"
msgid "Play touch tones when dialing"
msgstr "השמע צלילי מגע בעת החיוג"

msgctxt "dial_press_vibrate"
msgid "Dialing vibrate feedback"
msgstr "תגובת רטט לחיוג"

msgctxt "dial_press_vibrate_desc"
msgid "Haptic (vibrate) feedback when dialing"
msgstr "זז (רוטט) בתגובה בעת החיוג"

msgctxt "start_with_text_dialer"
msgid "Text dialer"
msgstr "חייגן טקסט"

msgctxt "start_with_text_dialer_desc"
msgid "Start with text dialer"
msgstr "התחל עם חייגן הטקסט"

msgctxt "use_alternate_unlocker"
msgid "Don't use slide answer"
msgstr "אל תשתמש לענות בגלישה"

msgctxt "use_alternate_unlocker_desc"
msgid "Use buttons instead of slide to take incoming calls"
msgstr "בלחצנים השתמש במקום בגלישה כדי לקבל שיחות נכנסות"

msgctxt "keep_awake_incall"
msgid "Keep awake while on call"
msgstr "הישאר ער בזמן שיחה"

msgctxt "keep_awake_incall_desc"
msgid "Fixes if you get choppy sound when screen turn off while in WIFI call"
msgstr "מתקן אם אתה מקבל קול קטוע כאשר המסך כבה בעת שיחת WIFI"

msgctxt "use_partial_wake_lock"
msgid "Use partial wake lock"
msgstr "השתמש במנעול מתעורר חלקי"

msgctxt "use_partial_wake_lock_desc"
msgid "Ensure CPU is always running for the application (drain battery)"
msgstr "ודא שהמעבד זמין תמיד ופועל עבור היישום (מרוקן את הסוללה)"

msgctxt "log_level"
msgid "Log level"
msgstr "שלב ההתחברות"

msgctxt "log_level_desc"
msgid "Log level for debug between 0 (none) and 5 (verbose)"
msgstr "שלב ההתחברות עבור debug בין 0 (לא) ו-5 (מפורט)"

msgctxt "integrate_with_native_music"
msgid "Integrate with Music application"
msgstr "שלב עם מוזיקת היישום"

msgctxt "integrate_with_native_music_desc"
msgid "Disable it if you use another music application than the official one"
msgstr "בטל את זה אם אתה משתמש ביישום אחר למוזיקה מזו הרשמית"

msgctxt "ringtone"
msgid "Ringtone"
msgstr "צליל"

msgctxt "ringtone_desc"
msgid "Default ringtone for SIP calls"
msgstr "צליל ברירת מחדל עבור שיחות סיפ"

msgctxt "prevent_screen_rotation"
msgid "Prevent screen rotation"
msgstr "מנע סיבוב המסך"

msgctxt "prevent_screen_rotation_desc"
msgid "Avoid rotation of the screen and keep in portrait mode"
msgstr "הימנע מסיבוב המסך ולשמור במצב דיוקן"

msgctxt "gsm_integration_type"
msgid "Mobile phone integration"
msgstr "שילוב סלולרי בטלפון"

msgctxt "gsm_integration_type_desc"
msgid "How the application uses your mobile phone (or not)"
msgstr "כיצד היישום משתמש (או שלא) בטלפון הנייד שלך"

msgctxt "invert_proximity_sensor"
msgid "Invert proximity sensor"
msgstr "הפוך קירבה חיישן"

msgctxt "invert_proximity_sensor_desc"
msgid "May help on some devices when manufacturer inverted near and far"
msgstr "עשוי לעזור על כמה מכשירים, כאשר יצרנית הפכה קרוב ורחוק"

msgctxt "theme"
msgid "Theme"
msgstr "ערכת נושא"

msgctxt "theme_desc"
msgid "Theme for the application"
msgstr "ערכת נושא עבור היישום"

msgctxt "obtain_themes"
msgid "Obtain more themes"
msgstr "קבל ערכות נושא נוספים"

msgctxt "obtain_themes_desc"
msgid "Get themes apps on the market"
msgstr "קבל ערכות נושא היישומים במרקט"

msgctxt "display_icon_in_status_bar_nbr"
msgid "Display number in status bar"
msgstr "הצג מספר בשורת המצב"

msgctxt "display_icon_in_status_bar_nbr_desc"
msgid "Display the number of registered accounts in status bar"
msgstr "הצג את מספר החשבונות הרשומים בשורת המצב"

#. Fast settings
msgctxt "android_integration"
msgid "Integrate with Android"
msgstr "שלב עם אנדרואיד"

msgctxt "android_integration_desc"
msgid "Integration with native dialer and call logs"
msgstr "שלב עם החייגן ויומן השיחות המקורי"

msgctxt "availability_profile"
msgid "Availability profile"
msgstr "פרופיל זמין"

msgctxt "always_available"
msgid "Always available"
msgstr "תמיד זמין"

msgctxt "always_available_desc"
msgid "Will use battery but incoming calls are always received"
msgstr "יעשה שימוש בסוללה אבל שיחות נכנסות מתקבלות תמיד"

msgctxt "wifi_available"
msgid "Available on WiFi"
msgstr "זמין ב WiFi"

msgctxt "wifi_available_desc"
msgid "Availability of incoming calls based on WiFi profile"
msgstr "הזמינות של שיחות נכנסות מבוסס על פרופיל WiFi"

msgctxt "never_available"
msgid "Only for outgoing"
msgstr "רק בשביל יוצאות"

msgctxt "never_available_desc"
msgid "Activate the SIP client automatically when necessary"
msgstr "הפעל את לקוח הסיפ באופן אוטומטי בעת הצורך"

msgctxt "availability_tg"
msgid "Mobile data availability"
msgstr "זמינות נתונים סלולרי"

msgctxt "tg_allowed"
msgid "I'm allowed to use mobile"
msgstr "מותר לי להשתמש בסלולרי"

msgctxt "tg_allowed_desc"
msgid "Your carrier allows VoIP over your mobile data connection"
msgstr "הספק שלך מאפשר VoIP באמצעות חיבור הנתונים הסלולרי שלך"

msgctxt "codecs_availability_text"
msgid "Only available if sip is currently registered"
msgstr "זמין רק אם הסיפ נמצא רשום כרגע"

#. Calls settings
msgctxt "auto_record_calls"
msgid "Auto record calls"
msgstr "הקלט שיחות אוטומטי"

msgctxt "auto_record_calls_desc"
msgid "Record calls as soon as connected"
msgstr "הקלט שיחות מיד ברגע ההתחברות"

msgctxt "support_multiple_calls"
msgid "Support multiple calls"
msgstr "תמיכה במספר שיחות"

msgctxt "support_multiple_calls_desc"
msgid "Allow conference and multiple concurrent calls"
msgstr "אפשר ועידה ומספר שיחות בו זמנית"

msgctxt "expert_preferences"
msgid "Expert mode"
msgstr "מצב מומחה"

msgctxt "advanced_preferences"
msgid "Advanced mode"
msgstr "מצב מתקדם"

