msgctxt "prefs"
msgid "Settings"
msgstr "Налаштування"

msgctxt "prefs_fast"
msgid "Easy configuration"
msgstr "Проста конфігурація"

msgctxt "prefs_fast_desc"
msgid "Quick options to use if you don't know how SIP works"
msgstr "Швидкий варіант використання, якщо ви не знаєте, як SIP працює"

msgctxt "prefs_network"
msgid "Network"
msgstr "Мережа"

msgctxt "prefs_network_desc"
msgid "How SIP can be connected to the network"
msgstr "Як SIP може бути підключений до мережі"

msgctxt "prefs_media"
msgid "Media"
msgstr "Носії"

msgctxt "prefs_media_desc"
msgid "Codecs and in-call sound behavior"
msgstr "Кодеки і поведінка під час дзвінка"

msgctxt "prefs_ui"
msgid "User interface"
msgstr "Інтерфейс користувача"

msgctxt "prefs_ui_desc"
msgid "Tweak the user interface"
msgstr "Настроювання користувацького інтерфейсу"

#. Network
msgctxt "for_incomings"
msgid "For incoming calls"
msgstr "Для вхідних дзвінків"

msgctxt "for_outgoings"
msgid "For outgoing calls"
msgstr "Для вихідних дзвінків"

msgctxt "perfs"
msgid "Performances"
msgstr "Швидкодія"

msgctxt "transport"
msgid "Transport"
msgstr "Транспорт"

msgctxt "transport_desc"
msgid "Force the use of a transport for this account"
msgstr "Вибрати транспорт даних для цього акаунта"

msgctxt "use_wifi"
msgid "Use WiFi"
msgstr "Використовувати WiFi"

msgctxt "use_wifi_in_desc"
msgid "Use WiFi for incoming calls"
msgstr "Використовувати WiFi для вхідних дзвінків"

msgctxt "use_wifi_out_desc"
msgid "Use WiFi for outgoing calls"
msgstr "Використовувати WiFi для вихідних дзвінків"

msgctxt "use_other"
msgid "Use other networks"
msgstr "Використовувати інші мережі"

msgctxt "use_other_in_desc"
msgid "For example WiMax for incoming calls"
msgstr "Для прикладу WiMax для вхідних дзвінків"

msgctxt "use_other_out_desc"
msgid "For example WiMax for outgoing calls"
msgstr "Для прикладу WiMax для вихідних дзвінків"

msgctxt "use_3g"
msgid "Use 3G"
msgstr "Використовувати 3G"

msgctxt "use_3g_disclamer"
msgid "Your carrier MUST allow you to use this option"
msgstr "Ваш оператор повинен дозволяти використовувати цю опцію"

msgctxt "use_edge"
msgid "Use EDGE"
msgstr "Використовувати EDGE"

msgctxt "use_gprs"
msgid "Use GPRS"
msgstr "Використовувати GPRS"

msgctxt "lock_wifi"
msgid "Lock WiFi"
msgstr "Блокувати WiFi"

msgctxt "lock_wifi_desc"
msgid "Prevent WiFi idle when SIP is connected"
msgstr "Запобігання WiFi простою, коли SIP з'єднано"

msgctxt "enable_tcp"
msgid "TCP"
msgstr "TCP"

msgctxt "enable_tcp_desc"
msgid "Enable TCP transport"
msgstr "Включити TCP транспорт"

msgctxt "enable_tls"
msgid "TLS"
msgstr "TLS"

msgctxt "enable_tls_desc"
msgid "Enable TLS transport"
msgstr "Включити TLS транспорт"

msgctxt "enable_udp"
msgid "UDP"
msgstr "UDP"

msgctxt "enable_udp_desc"
msgid "Enable UDP transport"
msgstr "Включити UDP транспорт"

msgctxt "network_udp_transport_port"
msgid "UDP Port"
msgstr "UDP порт"

msgctxt "network_udp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Номер порту для прив'язки локально"

msgctxt "network_tcp_transport_port"
msgid "TCP Port"
msgstr "Порт TCP"

msgctxt "network_tcp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Номер порту для прив'язки локально"

msgctxt "network_rtp_port"
msgid "RTP port"
msgstr "RTP порт"

msgctxt "network_rtp_port_desc"
msgid "Port for RTP ports range start"
msgstr "Порт для RTP діапазону"

msgctxt "use_ipv6"
msgid "Use IPv6"
msgstr "Використовувати IPv6"

msgctxt "use_ipv6_desc"
msgid "Switch to IPv6 mode (your network and phone must support it)"
msgstr ""
"Переключення в режим IPv6 (мережа і телефон повинен підтримувати його)"

msgctxt "enable_dns_srv"
msgid "Resolve DNS SRV"
msgstr "Отримати DNS SRV"

msgctxt "enable_dns_srv_desc"
msgid "Use the application resolver to resolve DNS names"
msgstr "Використання програми арбітра для вирішення DNS імен"

msgctxt "override_nameserver"
msgid "Use a custom DNS"
msgstr "Використовувати свій DNS"

msgctxt "override_nameserver_desc"
msgid "Left blank to retrieve DNS from Android"
msgstr "Залиште поле порожнім, щоб отримати DNS від Android"

msgctxt "nat_traversal"
msgid "NAT traversal"
msgstr "NAT обхід"

msgctxt "enable_ice"
msgid "Enable ICE"
msgstr "Включити ICE"

msgctxt "enable_ice_desc"
msgid "Turn on ICE feature"
msgstr "Увімкнути функцію ICE"

msgctxt "enable_turn"
msgid "Enable TURN"
msgstr "Включити TURN"

msgctxt "enable_turn_desc"
msgid "Turn on TURN feature"
msgstr "Увімкнути функцію TURN"

msgctxt "turn_server"
msgid "Turn server"
msgstr "Turn сервер"

msgctxt "turn_server_desc"
msgid "TURN server (HOST:PORT)"
msgstr "TURN сервер (ХОСТ:ПОРТ)"

msgctxt "enable_stun"
msgid "Enable STUN"
msgstr "Включити STUN"

msgctxt "enable_stun_desc"
msgid "Turn on STUN feature"
msgstr "Увімкнути функцію STUN"

msgctxt "stun_server"
msgid "STUN server"
msgstr "STUN сервер"

msgctxt "stun_server_desc"
msgid "STUN server (HOST:PORT)"
msgstr "STUN сервер (ХОСТ:ПОРТ)"

msgctxt "enable_qos"
msgid "Enable QoS"
msgstr "Включити QoS"

msgctxt "enable_qos_desc"
msgid "Enable QoS on packets"
msgstr "Увімкнути QoS на пакетах"

msgctxt "dscp_val"
msgid "DSCP value"
msgstr "DSCP значення"

msgctxt "dscp_val_desc"
msgid "DSCP value for SIP traffic"
msgstr "DSCP значення для SIP трафіку"

#. Security
msgctxt "tls"
msgid "TLS"
msgstr "TLS"

msgctxt "secure_transport"
msgid "Secure transport"
msgstr "Захищений транспорт"

msgctxt "secure_transport_desc"
msgid "Security mechanism for SIP and RTP"
msgstr "Механізм забезпечення безпеки для SIP і RTP"

msgctxt "secure_media"
msgid "Secure media"
msgstr "Захищені медіа"

msgctxt "network_tls_transport_port"
msgid "TLS Port"
msgstr "TLS порт"

msgctxt "network_tls_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Номер порту для прив'язки локально"

msgctxt "network_tls_server_name"
msgid "TLS server name"
msgstr "TLS ім'я сервера"

msgctxt "network_tls_server_name_desc"
msgid "For multi-hosting server (optional)"
msgstr "Для мульти-хостинг серверів (необов'язково)"

msgctxt "ca_list_file"
msgid "TLS CA file"
msgstr "TLS CA файл"

msgctxt "ca_list_file_desc"
msgid "For example /sdcard/CSipSimple/myAuthority.ca"
msgstr "Наприклад /sdcard/CSipSimple/myAuthority.ca"

msgctxt "cert_file"
msgid "TLS certificate file"
msgstr "TLS файл сертифікату"

msgctxt "cert_file_desc"
msgid "For example /sdcard/CSipSimple/myCert.crt"
msgstr "Наприклад /sdcard/CSipSimple/myCert.crt"

msgctxt "privkey_file"
msgid "TLS private key file"
msgstr "TLS файл приватного ключа"

msgctxt "privkey_file_desc"
msgid "For example /sdcard/CSipSimple/myKey.key"
msgstr "Наприклад /sdcard/CSipSimple/myKey.key"

msgctxt "tls_password"
msgid "TLS password"
msgstr "TLS пароль"

msgctxt "tls_password_desc"
msgid "TLS password to private key file"
msgstr "TLS пароль для файлу приватного ключа"

msgctxt "verify_server"
msgid "Check server"
msgstr "Перевірити сервер"

msgctxt "verify_server_desc"
msgid "Verify server's certificate"
msgstr "Перевірка сертифікату сервера"

msgctxt "verify_client"
msgid "Check client"
msgstr "Перевірити клієнта"

msgctxt "verify_client_desc"
msgid "Verify client's certificate"
msgstr "Перевірка сертифікату клієнта"

msgctxt "tls_method"
msgid "TLS Method"
msgstr "TLS метод"

msgctxt "tls_method_desc"
msgid "Method for TLS transport"
msgstr "Метод TLS транспорту"

msgctxt "use_srtp"
msgid "SRTP mode"
msgstr "SRTP режим"

msgctxt "use_srtp_desc"
msgid "Mode for Secure RTP"
msgstr "Режим для захищеного RTP"

#. Media
msgctxt "audio_quality"
msgid "Audio quality"
msgstr "Якість звуку"

msgctxt "audio_volume"
msgid "Volume"
msgstr "Гучність"

msgctxt "codecs"
msgid "Codecs"
msgstr "Кодеки"

msgctxt "echo_cancellation"
msgid "Echo cancellation"
msgstr "Придушення луни"

msgctxt "echo_cancellation_desc"
msgid "Activate echo cancellation"
msgstr "Активувати приглушення еха"

msgctxt "enable_vad"
msgid "Voice audio detection"
msgstr "Виявлення звуку"

msgctxt "enable_vad_desc"
msgid "Activate voice audio detection"
msgstr "Активація виявлення звуку"

msgctxt "snd_mic_level"
msgid "Micro amplification"
msgstr "Мікро посилення"

msgctxt "snd_mic_level_desc"
msgid "Adjust the micro signal level (1 do nothing)"
msgstr "Відрегулюйте мікро рівні сигналу (1 нічого не робити)"

msgctxt "snd_speaker_level"
msgid "Speaker amplification"
msgstr "Спікер посилення"

msgctxt "snd_speaker_level_desc"
msgid "Adjust the speaker signal level (1 do nothing)"
msgstr "Відрегулюйте рівень спікер сигналу (1 нічого не робити)"

msgctxt "snd_stream_level"
msgid "Initial stream level"
msgstr "Початковий рівень потоку"

msgctxt "snd_stream_level_desc"
msgid "Set the initial level for audio stream (Android stream)"
msgstr "Встановити початковий рівень для аудіо потоку (Android потік)"

msgctxt "snd_auto_close_time"
msgid "Auto close time"
msgstr "Час авто закриття"

msgctxt "snd_auto_close_time_desc"
msgid "Time to wait for close audio channel"
msgstr "Час очікування до закриття аудіо каналу"

msgctxt "snd_media_quality"
msgid "Media quality"
msgstr "Якість медіа"

msgctxt "snd_media_quality_desc"
msgid ""
"Quality of the media stream (modify only if you know what you are doing)"
msgstr ""
"Якість потоку засобів масової інформації (змінити, тільки якщо ви знаєте, що "
"робите)"

msgctxt "snd_clock_rate"
msgid "Clock rate"
msgstr "Тактова частота"

msgctxt "snd_clock_rate_desc"
msgid "Audio channel frequency"
msgstr "Аудіо частотний канал"

msgctxt "snd_ptime"
msgid "Frame ptime"
msgstr "Ptime пакету"

msgctxt "snd_ptime_desc"
msgid "Specifying lower ptime will normally reduce the latency"
msgstr "Зменшити ptime, як правило, зменшити латентність"

msgctxt "echo_cancellation_tail"
msgid "Echo cancellation tail"
msgstr "Придушення луни хвіст"

msgctxt "echo_cancellation_tail_desc"
msgid "Tail length of echo cancellation"
msgstr "Хвіст завдовжки луни"

msgctxt "codecs_desc"
msgid "Priority (0 is disabled)"
msgstr "Пріоритет (0 вимкнено)"

msgctxt "sip_dtmf"
msgid "DTMF mode"
msgstr "DTMF режим"

msgctxt "sip_dtmf_desc"
msgid "How to send DTMF"
msgstr "Як відіслати DTMF"

msgctxt "use_soft_volume"
msgid "Use software volume"
msgstr "Використання програмної гучності"

msgctxt "use_soft_volume_desc"
msgid "Use software volume instead of android volume"
msgstr "Використання програмного забезпечення програми, а не Android"

msgctxt "use_routing_api"
msgid "Use routing API"
msgstr "Використовувати API маршрутизації"

msgctxt "use_routing_api_desc"
msgid "Try to enable that if audio is routing to speaker instead of earpiece"
msgstr ""
"Спробуйте включити, що якщо звук маршрутизації оратор замість навушників"

msgctxt "use_mode_api"
msgid "Use Mode audio API"
msgstr "Використовувати режим аудіо API"

msgctxt "use_mode_api_desc"
msgid "Try to enable that if audio is routing to speaker instead of earpiece"
msgstr ""
"Спробуйте включити, що якщо звук маршрутизації оратор замість навушників"

msgctxt "sip_audio_mode"
msgid "Audio mode for SIP calls"
msgstr "Аудіо режим SIP дзвінків"

msgctxt "sip_audio_mode_desc"
msgid "Change only if developers advise you to"
msgstr "Зміна тільки тоді, коли розробники радять вам"

msgctxt "has_io_queue"
msgid "IO Queue"
msgstr ""

msgctxt "has_io_queue_desc"
msgid "Activate Queue for input/output"
msgstr ""

#. UI
msgctxt "general_ui"
msgid "General user interface"
msgstr "Користувальницький інтерфейс"

msgctxt "native_dialer_integration"
msgid "Native dialer integration"
msgstr "Тісна інтеграція дозвону"

msgctxt "advanced_ui"
msgid "Advanced tweaks"
msgstr "Додаткові налаштуваннях"

msgctxt "integrate_with_native_dialer"
msgid "Dialer integration"
msgstr "Інтеграція з системним додзвоном"

msgctxt "integrate_with_native_dialer_desc"
msgid ""
"When dialing with native dialer you'll be asked if you want to use SIP or "
"mobile"
msgstr ""
"При наборі з системного дозвону вам буде запропоновано, що ви хочете "
"використовувати SIP або мобільний"

msgctxt "integrate_with_native_calllogs"
msgid "Call logs integration"
msgstr "Інтеграція журналу дзвінків"

msgctxt "integrate_with_native_calllogs_desc"
msgid "SIP calls will be merged (if possible) into native call logs"
msgstr ""
"SIP-дзвінки будуть об'єднані (якщо можливо) у рідному журнали викликів"

msgctxt "dial_press_tone"
msgid "Dialing tone feedback"
msgstr "Гудок зворотного зв'язку"

msgctxt "dial_press_tone_desc"
msgid "Play touch tones when dialing"
msgstr "Слухати тонн при наборі"

msgctxt "dial_press_vibrate"
msgid "Dialing vibrate feedback"
msgstr "Вібрувати при зворотньому зв'язку"

msgctxt "dial_press_vibrate_desc"
msgid "Haptic (vibrate) feedback when dialing"
msgstr "Haptic (вібрувати) зворотний зв'язок при наборі"

msgctxt "start_with_text_dialer"
msgid "Text dialer"
msgstr "Текст дозвону"

msgctxt "start_with_text_dialer_desc"
msgid "Start with text dialer"
msgstr "Почати з текстового дозвону"

msgctxt "use_alternate_unlocker"
msgid "Don't use slide answer"
msgstr "Не використовувати слайд для відповіді"

msgctxt "use_alternate_unlocker_desc"
msgid "Use buttons instead of slide to take incoming calls"
msgstr "Використовуйте кнопки замість слайд щоб приймати вхідні дзвінки"

msgctxt "keep_awake_incall"
msgid "Keep awake while on call"
msgstr "Пильнувати у той час як за викликом"

msgctxt "keep_awake_incall_desc"
msgid "Fixes if you get choppy sound when screen turn off while in WIFI call"
msgstr ""
"Лагодження, якщо ви отримуєте поривчастий звук, коли екран вимкнути в той "
"час як в WIFI мережі"

msgctxt "use_partial_wake_lock"
msgid "Use partial wake lock"
msgstr "Використання часткового блокування"

msgctxt "use_partial_wake_lock_desc"
msgid "Ensure CPU is always running for the application (drain battery)"
msgstr ""
"Переконайтеся процесор завжди працює для додатків (батарея розрядиться "
"швидко)"

msgctxt "log_level"
msgid "Log level"
msgstr "Рівень журналу для налагодження"

msgctxt "log_level_desc"
msgid "Log level for debug between 0 (none) and 5 (verbose)"
msgstr "Рівень журналу для налагодження від 0 (немає) і 5 (детально)"

msgctxt "integrate_with_native_music"
msgid "Integrate with Music application"
msgstr "Інтеграція з музичним додатком"

msgctxt "integrate_with_native_music_desc"
msgid "Disable it if you use another music application than the official one"
msgstr "Вимкніть його, якщо ви використовуєте іншу музику, ніж стандартна"

msgctxt "ringtone"
msgid "Ringtone"
msgstr "Рингтон"

msgctxt "ringtone_desc"
msgid "Default ringtone for SIP calls"
msgstr "Рингтон по замовчуванню для SIP дзвінків"

msgctxt "prevent_screen_rotation"
msgid "Prevent screen rotation"
msgstr "Заборонити повертання екрану"

msgctxt "prevent_screen_rotation_desc"
msgid "Avoid rotation of the screen and keep in portrait mode"
msgstr "Уникнути обертання екрану і зберегти в портретному режимі"

#. Fast settings
msgctxt "android_integration"
msgid "Integrate with Android"
msgstr "Інтеграція з Android"

msgctxt "android_integration_desc"
msgid "Integration with native dialer and call logs"
msgstr "Інтеграція з рідним номеронабирачем і журналом викликів"

msgctxt "availability_profile"
msgid "Availability profile"
msgstr "Наявність профілю"

msgctxt "always_available"
msgid "Always available"
msgstr "Завжди доступний"

msgctxt "always_available_desc"
msgid "Will use battery but incoming calls are always received"
msgstr ""
"Буде використовувати батарею, але вхідні дзвінки завжди отримуватимуться"

msgctxt "wifi_available"
msgid "Available on WiFi"
msgstr "Доступний коли в мережі WiFi"

msgctxt "wifi_available_desc"
msgid "Availability of incoming calls based on WiFi profile"
msgstr "Наявні вхідні дзвінки на основі WiFi профілю"

msgctxt "never_available"
msgid "Only for outgoing"
msgstr "Тільки для вихідних дзвінків"

msgctxt "never_available_desc"
msgid "Activate the SIP client automatically when necessary"
msgstr "Включити клієнт SIP автоматично при необхідності"

msgctxt "availability_tg"
msgid "Mobile data availability"
msgstr "Доступність даних в мобільній мережі"

msgctxt "tg_allowed"
msgid "I'm allowed to use mobile"
msgstr "Мені дозволено користуватися мобільними"

msgctxt "tg_allowed_desc"
msgid "Your carrier allows VoIP over your mobile data connection"
msgstr "Ваш оператор дозволяє VoIP"

msgctxt "codecs_availability_text"
msgid "Only available if sip is currently registered"
msgstr "Доступно тільки якщо SIP в даний час зареєстровано"

msgctxt "expert_preferences"
msgid "Expert mode"
msgstr "Режим експерта"

msgctxt "advanced_preferences"
msgid "Advanced mode"
msgstr "Розширений режим"

