msgctxt "prefs"
msgid "Settings"
msgstr "Impostazioni"

msgctxt "prefs_fast"
msgid "Easy configuration"
msgstr "Configurazione facile"

msgctxt "prefs_fast_desc"
msgid "Quick options to use if you don't know how SIP works"
msgstr "Opzioni rapide da utilizzare se non si sa come funziona SIP"

msgctxt "prefs_network"
msgid "Network"
msgstr "Rete"

msgctxt "prefs_network_desc"
msgid "How SIP can be connected to the network"
msgstr "Come Sip può connettersi alla rete"

msgctxt "prefs_media"
msgid "Media"
msgstr "Audio"

msgctxt "prefs_media_desc"
msgid "Codecs and in-call sound behavior"
msgstr "Codec e qualità del suono in chiamata"

msgctxt "prefs_ui"
msgid "User interface"
msgstr "Interfaccia utente"

msgctxt "prefs_ui_desc"
msgid "Tweak the user interface"
msgstr "Tweak interfaccia utente"

msgctxt "for_incomings"
msgid "For incoming calls"
msgstr "Per chiamate in ingresso"

msgctxt "for_outgoings"
msgid "For outgoing calls"
msgstr "Per chiamate in uscita"

msgctxt "perfs"
msgid "Performances"
msgstr "Prestazioni"

msgctxt "transport"
msgid "Transport"
msgstr "Trasporto"

msgctxt "use_wifi"
msgid "Use WiFi"
msgstr "Usa wifi"

msgctxt "use_wifi_in_desc"
msgid "Use WiFi for incoming calls"
msgstr "Usa wifi per chiamate in ingresso"

msgctxt "use_wifi_out_desc"
msgid "Use WiFi for outgoing calls"
msgstr "Usa wifi per chiamate in uscita"

msgctxt "use_other"
msgid "Use other networks"
msgstr "Usa altre reti"

msgctxt "use_other_in_desc"
msgid "For example WiMax for incoming calls"
msgstr "Per esempio WiMax per chiamate in ingresso"

msgctxt "use_other_out_desc"
msgid "For example WiMax for outgoing calls"
msgstr "Per esempio WiMax per chiamate in uscita"

msgctxt "use_3g"
msgid "Use 3G"
msgstr "Usa 3G"

msgctxt "use_3g_disclamer"
msgid "Your carrier MUST allow you to use this option"
msgstr "La tua compagnia telefonica DEVE consentire questa opzione"

msgctxt "use_edge"
msgid "Use EDGE"
msgstr "Usa EDGE"

msgctxt "use_gprs"
msgid "Use GPRS"
msgstr "Usa GPRS"

msgctxt "lock_wifi"
msgid "Lock WiFi"
msgstr "Blocca wifi"

msgctxt "lock_wifi_desc"
msgid "Prevent WiFi idle when SIP is connected"
msgstr "Evita la dissattivazione del wifi quando SIP è connesso"

msgctxt "enable_tcp"
msgid "TCP"
msgstr "TCP"

msgctxt "enable_tcp_desc"
msgid "Enable TCP transport"
msgstr "Abilita trasporto TCP"

msgctxt "enable_tls"
msgid "TLS"
msgstr "TLS"

msgctxt "enable_tls_desc"
msgid "Enable TLS transport"
msgstr "Abilita il trasporto TLS"

msgctxt "enable_udp"
msgid "UDP"
msgstr "UDP"

msgctxt "enable_udp_desc"
msgid "Enable UDP transport"
msgstr "Abilita trasporto UDP"

msgctxt "network_udp_transport_port"
msgid "UDP Port"
msgstr "Porta UDP"

msgctxt "network_udp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numero di porta da impegnare in locale"

msgctxt "network_tcp_transport_port"
msgid "TCP Port"
msgstr "Porta TCP"

msgctxt "network_tcp_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numero di porta da impegnare in locale"

msgctxt "network_tls_transport_port"
msgid "TLS Port"
msgstr "Porta TLS"

msgctxt "network_tls_transport_port_desc"
msgid "Port number to bind locally"
msgstr "Numero di porta da impegnare in locale"

msgctxt "network_rtp_port"
msgid "RTP port"
msgstr "Porta RTP"

msgctxt "network_rtp_port_desc"
msgid "Port for RTP ports range start"
msgstr "Porta RTP di partenza per l'intervallo"

msgctxt "use_ipv6"
msgid "Use IPv6"
msgstr "Usa IPv6"

msgctxt "use_ipv6_desc"
msgid "Switch to IPv6 mode (your network and phone must support it)"
msgstr "Passa alla modalità IPv6 (la rete e il telefono devono supportarlo)"

msgctxt "enable_dns_srv"
msgid "Resolve DNS SRV"
msgstr "Risolvi DNS SRV"

msgctxt "enable_dns_srv_desc"
msgid "Use CSipSimple nameserver resolver resolve names"
msgstr "Usa CSipSimple per risolvere i nomi"

msgctxt "override_nameserver"
msgid "Use a custom DNS"
msgstr "Usa un DNS personalizzato"

msgctxt "override_nameserver_desc"
msgid "Left blank to retrieve DNS from Android"
msgstr "Lasciare in bianco per recuperare DNS da Android"

msgctxt "audio_quality"
msgid "Audio quality"
msgstr "Qualità audio"

msgctxt "codecs"
msgid "Codecs"
msgstr "Codec"

msgctxt "ice_turn"
msgid "ICE/TURN/STUN"
msgstr "ICE/TURN/STUN"

msgctxt "echo_cancellation"
msgid "Echo cancellation"
msgstr "Cancellazione echo"

msgctxt "echo_cancellation_desc"
msgid "Activate echo cancellation"
msgstr "Attiva la cancellazione dell'echo"

msgctxt "enable_vad"
msgid "Voice audio detection"
msgstr "Rilevamento della voce"

msgctxt "enable_vad_desc"
msgid "Activate voice audio detection"
msgstr "Attiva il rilevamento della voce"

msgctxt "snd_mic_level"
msgid "Micro amplification"
msgstr "Amplificazione del microfono"

msgctxt "snd_mic_level_desc"
msgid "Adjust the micro signal level (1 do nothing)"
msgstr "Regola il livello di segnale del microfono  (1 non fa nulla)"

msgctxt "snd_speaker_level"
msgid "Speaker amplification"
msgstr "Amplificazione dell'altoparlante"

msgctxt "snd_speaker_level_desc"
msgid "Adjust the speaker signal level (1 do nothing)"
msgstr "Regola il livello di segnale dell'altoparlante  (1 non fa nulla)"

msgctxt "snd_stream_level"
msgid "Initial stream level"
msgstr "Livello stream iniziale"

msgctxt "snd_stream_level_desc"
msgid "Set the initial level for audio stream (Android stream)"
msgstr "Imposta il livello iniziale per lo stream audio (stream Android)"

msgctxt "snd_auto_close_time"
msgid "Auto close time"
msgstr "Tempo di chiusura automatica"

msgctxt "snd_auto_close_time_desc"
msgid "Time to wait for close audio channel"
msgstr "Tempo di attesa per la chiusura del canale audio"

msgctxt "snd_media_quality"
msgid "Media quality"
msgstr "Qualità audio"

msgctxt "snd_media_quality_desc"
msgid ""
"Quality of the media stream (modify only if you know what you are doing)"
msgstr "Qualità dell'audio (modifica solo se sai cosa stai facendo)"

msgctxt "snd_clock_rate"
msgid "Clock rate"
msgstr "Clock rate"

msgctxt "snd_clock_rate_desc"
msgid "Audio channel frequency"
msgstr "Frequenza del canale audio"

msgctxt "snd_ptime"
msgid "Frame ptime"
msgstr "Frame ptime"

msgctxt "snd_ptime_desc"
msgid "Specifying lower ptime will normally reduce the latency"
msgstr "Specificare un ptime più basso normalmente riduce la latenza"

msgctxt "echo_cancellation_tail"
msgid "Echo cancellation tail"
msgstr "Coda di cancellazione dell'eco"

msgctxt "echo_cancellation_tail_desc"
msgid "Tail length of echo cancellation"
msgstr "Lunghezza della coda di cancellazione dell'eco"

msgctxt "enable_ice"
msgid "Enable ICE"
msgstr "Abilita ICE"

msgctxt "enable_ice_desc"
msgid "Turn on ICE feature"
msgstr "Attiva ICE"

msgctxt "enable_turn"
msgid "Enable TURN"
msgstr "Attiva TURN"

msgctxt "enable_turn_desc"
msgid "Turn on TURN feature"
msgstr "Attiva TURN"

msgctxt "turn_server"
msgid "Turn server"
msgstr "Turn server"

msgctxt "turn_server_desc"
msgid "TURN server (HOST:PORT)"
msgstr "Turn server (HOST:PORT)"

msgctxt "enable_stun"
msgid "Enable STUN"
msgstr "Abilita STUN"

msgctxt "enable_stun_desc"
msgid "Turn on STUN feature"
msgstr "Attiva funzionalità STUN"

msgctxt "stun_server"
msgid "STUN server"
msgstr "STUN server"

msgctxt "stun_server_desc"
msgid "STUN server (HOST:PORT)"
msgstr "STUN server (HOST:PORT)"

msgctxt "codecs_desc"
msgid "Priority (0 is disabled)"
msgstr "Priorità (0 disabilita)"

msgctxt "use_srtp"
msgid "SRTP mode"
msgstr "Modalità SRTP"

msgctxt "use_srtp_desc"
msgid "Mode for Secure RTP"
msgstr "Modalità per RTP Sicuro"

msgctxt "general_ui"
msgid "General user interface"
msgstr "Interfaccia utente generale"

msgctxt "native_dialer_integration"
msgid "Native dialer integration"
msgstr "Integrazione con il dialer nativo"

msgctxt "advanced_ui"
msgid "Advanced tweaks"
msgstr "Tweaks avanzati"

msgctxt "integrate_with_native_dialer"
msgid "Dialer integration"
msgstr "Integrazione dialer"

msgctxt "integrate_with_native_dialer_desc"
msgid ""
"When dialing with native dialer you'll be asked if you want to use SIP or "
"mobile"
msgstr ""
"Quando componi il numero con il dialer nativo ti verrà chiesto se vuoi usare "
"SIP o GSM"

msgctxt "integrate_with_native_calllogs"
msgid "Call logs integration"
msgstr "Integrazione con il log delle chiamate nativo"

msgctxt "integrate_with_native_calllogs_desc"
msgid "SIP calls will be merged (if possible) into native call logs"
msgstr ""
"Le chiamate SIP verranno inserite (se possibile) nel log nativo delle "
"chiamate"

msgctxt "exclude_for_outgoing_call"
msgid "Exclude patterns"
msgstr "Escludi patterns"

msgctxt "exclude_for_outgoing_call_desc"
msgid "Exclude patterns for outgoing calls"
msgstr "Escludi patterns per chiamate in uscita"

msgctxt "dial_press_tone"
msgid "Dialing tone feedback"
msgstr "Toni in composizione"

msgctxt "dial_press_tone_desc"
msgid "Play touch tones when dialing"
msgstr "Ascolta i toni durante la composizione"

msgctxt "dial_press_vibrate"
msgid "Dialing vibrate feedback"
msgstr "Vibrazione in composizione del numero"

msgctxt "dial_press_vibrate_desc"
msgid "Haptic (vibrate) feedback when dialing"
msgstr "Vibrazione durante la composizione del numero"

msgctxt "start_with_text_dialer"
msgid "Text dialer"
msgstr "Dialer testuale"

msgctxt "start_with_text_dialer_desc"
msgid "Start with text dialer"
msgstr "Inizia con il dialer testuale"

msgctxt "use_alternate_unlocker"
msgid "Don't use slide unlocker"
msgstr "Non usare sblocco scorrevole"

msgctxt "use_alternate_unlocker_desc"
msgid "Use buttons instead of slide to take incoming calls"
msgstr ""
"Usa i bottoni invece dello slide per accettare le chiamate in ingresso"

msgctxt "keep_awake_incall"
msgid "Keep awake while on call"
msgstr "Resta attivo durante una chiamata"

msgctxt "keep_awake_incall_desc"
msgid "Fixes if you get choppy sound when screen turn off while in WIFI call"
msgstr ""
"Corregge se si ottiene suono disturbato quando lo schermo si spegne durante "
"una chiamata WIFI"

msgctxt "use_partial_wake_lock"
msgid "Use partial wake lock"
msgstr "Usa partial wake lock"

msgctxt "use_partial_wake_lock_desc"
msgid "Ensure CPU is always running for CSipSimple (drain battery)"
msgstr ""
"Assicura che la CPU sia sempre in esecuzione per CSipSimple (scarica la "
"batteria)"

msgctxt "log_level"
msgid "Log level"
msgstr "Livello di log"

msgctxt "log_level_desc"
msgid "Log level for debug between 0 (none) and 5 (verbose)"
msgstr "Livello di debug tra 0 (nessuno) e 5 (dettagliato)"

msgctxt "integrate_with_native_music"
msgid "Integrate with Music application"
msgstr "Integrazione con l'applicazione Musica"

msgctxt "integrate_with_native_music_desc"
msgid "Disable it if you use another music application than the official one"
msgstr ""
"Disabilitare se si usa un'altra applicazione musicale diversa da quella "
"ufficiale"

msgctxt "android_integration"
msgid "Integrate with Android"
msgstr "Integrazione con Android"

msgctxt "android_integration_desc"
msgid "Integration with native dialer and call logs"
msgstr "Integrazione con il dialer nativo e log delle chiamate"

msgctxt "availability_profile"
msgid "Availability profile"
msgstr "Profilo di disponibilità"

msgctxt "always_available"
msgid "Always available"
msgstr "Sempre disponbile"

msgctxt "always_available_desc"
msgid "Will use battery but incoming calls are always received"
msgstr ""
"Utilizzerà la batteria ma le chiamate in ingresso sono sempre ricevibili"

msgctxt "wifi_available"
msgid "Available on WiFi"
msgstr "Disponibile su WIFI"

msgctxt "wifi_available_desc"
msgid "Availability of incoming calls based on WiFi profile"
msgstr "Disponibilità delle chiamate in ingresso basata sul profilo WIFI"

msgctxt "never_available"
msgid "Only for outgoing"
msgstr "Solo in uscita"

msgctxt "never_available_desc"
msgid "Activate the SIP client automatically when necessary"
msgstr "Attiverà il client SIP automaticamente quando necessario"

msgctxt "availability_tg"
msgid "Mobile data availability"
msgstr "Disponibilità connessione dati mobile"

msgctxt "tg_allowed"
msgid "I'm allowed to use mobile"
msgstr "Mi è consentito usare la connessione dati mobile"

msgctxt "tg_allowed_desc"
msgid "Your carrier allows VoIP over your mobile data connection"
msgstr ""
"Il tuo operatore telefonico permette l'uso della connessione dati mobile per "
"il VoIP"

msgctxt "codecs_availability_text"
msgid "Only available if sip is currently registered"
msgstr "Disponibile solo se sip è registrato"
