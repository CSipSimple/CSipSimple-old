#. The name of the SMS/MMS app as it appears to the user in the Home screen,
#. task switcher, etc.
msgctxt "app_label"
msgid "Messaging"
msgstr "短信"

#. Command name to open a Compose screen; displays in conversation list
msgctxt "new_message"
msgid "New message"
msgstr "新建短息"

#. Summary text under "New Message" command; displays in conversation list
msgctxt "create_new_message"
msgid "Compose new message"
msgstr "编写短信"

#. Menu item for starting a new conversation
msgctxt "menu_compose_new"
msgid "Compose"
msgstr "编写"

#, c-format
msgctxt "menu_add_address_to_contacts"
msgid "Add <xliff:g id=\"name\">%s</xliff:g> to Contacts"
msgstr "添加 <xliff:g id=\"name\">%s</xliff:g> 到联系人"

#. Menu item to call the recipient of the conversation on the phone
#. (imperative verb)
msgctxt "menu_call"
msgid "Call"
msgstr "呼叫"

#. Menu item for deleting all conversations
msgctxt "menu_delete_all"
msgid "Delete threads"
msgstr "删除所有会话"

#. Context menu item on an individual conversation for deleting that
#. conversation
msgctxt "menu_delete"
msgid "Delete thread"
msgstr "删除会话"

#. Context menu item on an individual conversation for viewing that
#. conversation
msgctxt "menu_view"
msgid "View thread"
msgstr "查看会话"

#. Name to use when we have an unspecified sender
msgctxt "anonymous_recipient"
msgid "Anonymous"
msgstr "匿名"

#. Name of the local user when displaying sender of an outgoing message
#. (personal pronoun)
msgctxt "messagelist_sender_self"
msgid "Me"
msgstr "我"

msgctxt "delete_message"
msgid "Delete message"
msgstr "删除短信"

#. Time and date a message was sent
#, c-format
msgctxt "sent_on"
msgid "Sent: <xliff:g id=\"date\">%s</xliff:g>"
msgstr "发送时间: <xliff:g id=\"date\">%s</xliff:g>"

#. Abbreviation for "kilobyte"
msgctxt "kilobyte"
msgid "KB"
msgstr "KB"

#. Dialog box title brought up when warning the user that a message could not
#. be delivered
msgctxt "undelivered_msg_dialog_title"
msgid "Undelivered message"
msgstr "短信无法送达"

#. Dialog box message indicating that a message could not be delivered
msgctxt "undelivered_sms_dialog_body"
msgid "Unable to send this message."
msgstr "无法发送此短信"

#. Menu item for deleting the entire conversation currently being viewed
msgctxt "delete_thread"
msgid "Delete thread"
msgstr "删除会话"

#. Text of a button appearing for MMS messages that have not yet been
#. downloaded, causing the message          to be downloaded (imperative verb)
msgctxt "download"
msgid "Download"
msgstr "下载"

#. Message displayed in place of the "download message" button once it has
#. been pressed,          indicating that the download is in progress
msgctxt "downloading"
msgid "Downloading"
msgstr "正在下载"

#. Formatting for prepending MMS message text with sender's name.
#, c-format
msgctxt "name_colon"
msgid "<b><xliff:g id=\"name\" example=\"Me\">%s</xliff:g></b>: "
msgstr "<b><xliff:g id=\"name\" example=\"Me\">%s</xliff:g></b>: "

#. Individual message context menu item
msgctxt "copy_message_text"
msgid "Copy message text"
msgstr "复制短信文本"

#. Header for context menu on an individual message
msgctxt "message_options"
msgid "Message options"
msgstr "短信选项"

#. Button text on edit slide screen indicating the user wants to finish
#. editing that slide
msgctxt "done"
msgid "Done"
msgstr "完成"

#. Title bar displayed in compose message activity before a recipient has been
#. selected
msgctxt "compose_title"
msgid "Compose"
msgstr "编写"

#. Menu item in conversation screen for attaching something to the message
#. being composed
msgctxt "add_attachment"
msgid "Attach"
msgstr "附加"

#. Button next to an attachment during message composition to view the
#. attachment (imperative verb)
msgctxt "view"
msgid "View"
msgstr "查看"

#. Button in conversation screen for sending the SMS message currently being
#. composed (imperative verb)
msgctxt "send"
msgid "Send"
msgstr "发送"

#. Menu item in conversation screen for returning to the conversation list
msgctxt "all_threads"
msgid "All threads"
msgstr "所有会话"

#. Hint text in message body field when keyboard is exposed
msgctxt "type_to_compose_text_enter_to_send"
msgid "Type to compose"
msgstr "打字编写短信"

#. Hint text in message body field when keyboard is hidden
msgctxt "open_keyboard_to_compose_message"
msgid "Open keyboard to compose message"
msgstr "打开键盘编写短信"

#. Dialog title for warning the user that the picture they are trying to
#. attach is too large
msgctxt "image_too_large"
msgid "Picture too large"
msgstr "图片太大"

#. Dialog message text asking the user if he wants to resize the picture being
#. attached
msgctxt "ask_for_automatically_resize"
msgid "Automatically resize picture before sending?"
msgstr "在发送前自动调整图片大小？"

#. Toast message while an image is being resized
msgctxt "compressing"
msgid "Picture too large. Compressing\\u2026"
msgstr "图片太大，正在压缩\\u2026"

#. Error message indicating the format for the specified media type (e.g.
#. "picture") is unsupported
#, c-format
msgctxt "unsupported_media_format"
msgid "Unsupported <xliff:g id=\"name\">%1$s</xliff:g> format"
msgstr "不支持<xliff:g id=\"name\">%1$s</xliff:g>格式"

#. Context menu item on a message recipient for viewing their contact info
msgctxt "menu_view_contact"
msgid "View contact"
msgstr "查看联系人"

#. Context menu item on a message recipient for adding them to the user's
#. contacts (used if the recipient is not already in the address book)
msgctxt "menu_add_to_contacts"
msgid "Add to Contacts"
msgstr "添加到联系人"

#. Sender to be used if the sender address has been hidden
msgctxt "hidden_sender_address"
msgid "Hidden sender address"
msgstr "隐藏发送人地址"

#. Text to be used for affirmative response button in dialog boxes
msgctxt "yes"
msgid "OK"
msgstr "确定"

#. Text to be used for negative response button in dialog boxes
msgctxt "no"
msgid "Cancel"
msgstr "取消"

#. Button text used for editing a slideshow (imperative verb)
msgctxt "edit"
msgid "Edit"
msgstr "修改"

#. Settings screen menu item for restoring all settings to default
msgctxt "restore_default"
msgid "Restore default settings"
msgstr "恢复默认设置"

#. Confirm Delete
#. Delete confirmation dialog title
msgctxt "confirm_dialog_title"
msgid "Delete"
msgstr "删除"

#. Delete entire thread confirmation dialog message
msgctxt "confirm_delete_conversation"
msgid "The entire thread will be deleted."
msgstr "将删除整个会话"

#. Delete all threads confirmation dialog message
msgctxt "confirm_delete_all_conversations"
msgid "All threads will be deleted."
msgstr "将删除所有会话"

#. Delete message confirmation dialog message
msgctxt "confirm_delete_message"
msgid "The message will be deleted."
msgstr "将删除短信"

#. Delete message confirmation dialog message for a locked message
msgctxt "confirm_delete_locked_message"
msgid "Are you sure you want to delete this locked message?"
msgstr "确认删除已锁短信？"

#. Delete entire thread confirmation dialog message
msgctxt "confirm_delete_all_messages"
msgid "The entire thread will be deleted."
msgstr "将删除整个会话"

#. Delete button title
msgctxt "delete"
msgid "Delete"
msgstr "删除"

#. Status in "Report" window
msgctxt "status_none"
msgid "(None)"
msgstr "(无)"

#. Status in "Report" window
msgctxt "status_pending"
msgid "Pending"
msgstr "待定中"

#. Status in "Report" window
msgctxt "status_received"
msgid "Received"
msgstr "已接收"

#. Status in "Report" window
msgctxt "status_failed"
msgid "Unsuccessful"
msgstr "失败"

#. Status in "Report" window
msgctxt "status_unread"
msgid "Unread"
msgstr "未读"

#. Status in "Report" window
msgctxt "status_rejected"
msgid "Rejected"
msgstr "已拒绝"

#. Label in "Report" window
msgctxt "recipient_label"
msgid "Recipient:\\u0020"
msgstr "收件人:\\u0020"

#. Label in "Report" window
msgctxt "status_label"
msgid "Status:\\u0020"
msgstr "状态:\\u0020"

#. Menu item
msgctxt "menu_insert_smiley"
msgid "Insert smiley"
msgstr "插入笑脸"

#. Label for the "Me" number of this phone.
msgctxt "me"
msgid "Me"
msgstr "我"

#. Brief small label shown to right of message when it is being sent.
msgctxt "sending_message"
msgid "Sending\\u2026"
msgstr "发送中\\u2026"

